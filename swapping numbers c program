#include <stdio.h>

int main()
{
    int a,b,temp;
    printf("Enter first element");
    scanf("%d",&a);
    printf("Enter second element");
    scanf("%d",&b);
    temp = a;
    a = b;
    b = temp;
    printf("after swaping,the value a is %d",a);
    printf("after swaping,the value b is %d",b);
    


    return 0;
}
